{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vtftk",
  "version": "0.0.0",
  "identifier": "com.jacobtread.vtftk",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "VTuber Fun ToolKit (VTFTK)",
        "width": 1100,
        "height": 625
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../defaults/": "defaults/"
    }
  }
}